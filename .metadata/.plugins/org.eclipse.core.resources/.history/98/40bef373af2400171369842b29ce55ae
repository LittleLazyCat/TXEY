<mapper namespace="com.txey.record.dao.mybatis.EmployeeMybatisDao">
	<select id="getEmployee" resultMap="EmpMap">
		SELECT ksid,ksmc,ksmcpy,kslxid,hcbz
		FROM tb_dept
	</select>
	<resultMap type="Employee" id="EmpMap">
		<constructor>
			<idArg column="ksid" javaType="int" />
			<arg column="ksmc" javaType="String" />
			<arg column="ksmcpy" javaType="String" />
			<arg column="kslxid" javaType="int" />
			<arg column="hcbz" javaType="String" />
		</constructor>
	</resultMap>
</mapper>